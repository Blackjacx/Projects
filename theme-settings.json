{
    "theme": {
        "aside": {
            "border-radius": "10pt",
            "border-style": "double",
            "border-width": "3px"
        },
        "border-radius": "10pt",
        "button": {
            "border-radius": "10pt",
            "border-style": "solid",
            "border-width": "2px"
        },
        "code": {
            "border-radius": "10pt",
            "border-style": "dashed",
            "border-width": "2px",
            "indentationWidth": 2
        },
        "color": {
            "primary": "#25252A",
            "secondary": "#2A2B30",
            "fill": {
                "dark": "var(--color-primary)",
                "light": "#FFFFFF"
            },
            "fill-secondary": {
                "dark": "var(--color-secondary)",
                "light": "#FFFFFF"
            },
            "documentation-intro-fill": "radial-gradient(circle at top, var(--color-fill-secondary) 30%, #151516 100%)",
            "documentation-intro-accent": "var(--color-secondary)",
            "nav-solid-background": "var(--color-fill-primary)"
        }
    }
}
